{
  "id": null,
  "uid": "shopsphere",
  "title": "ShopSphere - MLOps Overview",
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "tags": ["shopsphere", "mlops"],
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [
    {
      "type": "link",
      "title": "MLflow",
      "url": "http://localhost:5000",
      "targetBlank": true
    },
    {
      "type": "link",
      "title": "Prefect UI",
      "url": "http://localhost:4200",
      "targetBlank": true
    },
    {
      "type": "link",
      "title": "MinIO Console",
      "url": "http://localhost:9001",
      "targetBlank": true
    },
    {
      "type": "link",
      "title": "Prometheus",
      "url": "http://localhost:9090",
      "targetBlank": true
    },
    {
      "type": "link",
      "title": "Grafana",
      "url": "http://localhost:3000",
      "targetBlank": true
    }
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "label": "Job",
        "datasource": "Prometheus",
        "query": "label_values(up, job)",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "endpoint",
        "type": "query",
        "label": "API endpoint",
        "datasource": "Prometheus",
        "query": "label_values(api_requests_total, endpoint)",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 10,
      "type": "row",
      "title": "API - Traffic & Latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 1,
      "title": "API Requests /s by endpoint",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (endpoint) (rate(api_requests_total{endpoint=~\"$endpoint\"}[5m]))"
        }
      ]
    },
    {
      "id": 2,
      "title": "API Latency (ms) by endpoint",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (endpoint) (api_score_latency_ms{endpoint=~\"$endpoint\"})"
        }
      ]
    },

    {
      "id": 20,
      "type": "row",
      "title": "Model Health - CLV & Propensity",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 }
    },
    {
      "id": 3,
      "title": "CLV Drift Score",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.3 },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" },
      "targets": [{ "refId": "A", "expr": "clv_drift_score" }]
    },
    {
      "id": 4,
      "title": "Propensity AUC",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.65 },
              { "color": "green", "value": 0.75 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "propensity_auc" }]
    },
    {
      "id": 5,
      "title": "AUC Î” (current - reference)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": -0.05 },
              { "color": "orange", "value": -0.02 },
              { "color": "green", "value": 0 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "propensity_auc_delta" }]
    },
    {
      "id": 6,
      "title": "CLV Drift Score (timeseries)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 10 },
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
      "options": { "legend": { "displayMode": "hidden" } },
      "targets": [{ "refId": "A", "expr": "clv_drift_score" }]
    },

    {
      "id": 30,
      "type": "row",
      "title": "Recommender Quality",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 }
    },
    {
      "id": 7,
      "title": "Recommender Coverage (0..1)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 16 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0.2 },
              { "color": "orange", "value": 0.4 },
              { "color": "green", "value": 0.5 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "recommender_coverage" }]
    },
    {
      "id": 8,
      "title": "Recommender Novelty (avg inverse popularity)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 16, "x": 8, "y": 16 },
      "fieldConfig": { "defaults": { "unit": "none", "custom": { "scaleDistribution": { "type": "log", "log": 10 } } } },
      "options": { "legend": { "displayMode": "hidden" } },
      "targets": [{ "refId": "A", "expr": "recommender_novelty" }]
    },

    {
      "id": 40,
      "type": "row",
      "title": "Pricing Guardrails",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 }
    },
    {
      "id": 9,
      "title": "Violations (last run)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 0.001 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "pricing_guardrail_violations" }]
    },

    {
      "id": 50,
      "type": "row",
      "title": "Scrape & Service Health",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 }
    },
    {
      "id": 11,
      "title": "Targets up by job",
      "type": "bargauge",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 28 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": { "orientation": "horizontal", "displayMode": "lcd" },
      "targets": [
        { "refId": "A", "expr": "sum by (job) (up{job=~\"$job\"})" }
      ]
    },
    {
      "id": 12,
      "title": "API target up",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 4, "x": 12, "y": 28 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" },
      "targets": [{ "refId": "A", "expr": "sum(up{job=\"api\"})" }]
    },
    {
      "id": 13,
      "title": "Monitor Exporter up",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 4, "x": 16, "y": 28 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "sum(up{job=\"monitor-exporter\"})" }]
    },
    {
      "id": 14,
      "title": "Prometheus up",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 4, "x": 20, "y": 28 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "refId": "A", "expr": "sum(up{job=\"prometheus\"})" }]
    }
  ],
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["10s","30s","1m","5m","15m","30m","1h","3h","6h","12h","24h"]
  }
}
